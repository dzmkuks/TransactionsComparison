@{
    ViewData["Title"] = "Upload file";
}
<h1>@ViewData["Title"]</h1>

﻿
@{
    ViewData["Title"] = "ImportCSVFile";
}

<div class="container">
    <p>
        <a asp-controller="Home" asp-action="Index">Back to List</a>
    </p>
    <div class="row">
        <div class="col-6 offset-3">
            <form method="post" asp-action="UploadFile" asp-controller="Home" enctype="multipart/form-data">
                <div class="form-group">
                    <label>Upload Excel File :</label>
                    <input type="file" name="FormFile" class="form-control" accept=".csv,.xls,.xlsx" />
                </div>
                <input type="submit" class="btn btn-sm float-right btn-primary" value="Upload" />
            </form>
        </div>
    </div>
</div>

